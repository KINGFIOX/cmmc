int main() {
    int s = 0;
    // large
    for(int i = 0; i < 2000; ++i)
        s += i;
    // large with epilogue
    for(int i = 0; i < 101; ++i)
        s += i;
    // small
    for(int i = 0; i < 5; ++i)
        s += i;
    // small2
    for(int i = 0; i < 1; ++i)
        s += i;
    for(int i = 0; i < 0; ++i)
        s += i;
    for(int i = 0; i < -1; ++i)
        s += i;
    return s;
}
