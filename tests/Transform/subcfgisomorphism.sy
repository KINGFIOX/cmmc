int main() {
    int x = getint();
    while(x < 10) {
        while(x < 10) {
            if(x > 5)
                x = getint();
            else {
                while(x < 10) {
                    if(x > 5)
                        x = getint();
                    else {
                        while(x < 10) {
                            if(x > 5)
                                x = getint();
                            else {
                                while(x < 10) {
                                    if(x > 5)
                                        x = getint();
                                    else {
                                        while(x < 10) {
                                            if(x > 5)
                                                x = getint();
                                            else
                                                x = x + 1;
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
    return x;
}
