internal func @getint() -> i32 { NoMemoryRead NoMemoryWrite };
internal func @fac(i32 %a) -> i32 { NoMemoryRead NoMemoryWrite NoSideEffect Stateless } {
^entry:
    i1 %0 = scmp eq i32 %a, i32 1;
    cbr i1 %0(prob = 0.5), ^b, ^if.else;
^if.else:
    i32 %1 = add i32 %a, i32 -1;
    i32 %2 = call (i32) -> i32 @fac(i32 %1);
    i32 %3 = mul i32 %a, i32 %2;
    ubr ^b;
^b:
    i32 %4 = phi [^if.else, i32 %3] [^entry, i32 1];
    ret i32 %4;
}
func @main() -> i32 { NoMemoryRead NoMemoryWrite NoRecurse Entry } {
^entry:
    i32 %0 = call () -> i32 @getint();
    i32 %1 = call (i32) -> i32 @fac(i32 %0);
    ret i32 %1;
}
