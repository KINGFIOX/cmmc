int main() {
    int s = 0;
    // large
    for(int i = 0; i < 2000; ++i)
        s += i;
    // large with epilogue
    for(int i = 0; i < 101; ++i)
        s += i;
    // small
    for(int i = 0; i < 5; ++i)
        s += i;
    // small2
    for(int i = 0; i < 1; ++i)
        s += i;
    for(int i = 0; i < 0; ++i)
        s += i;
    for(int i = 0; i < -1; ++i)
        s += i;
    int i = 0;
    while(i < 15) {
        s += i;
        ++i;
    }
    i = 0;
    while(i < 16) {
        s += i * i;
        ++i;
    }
    i = 0;
    while(i < 32) {
        s += i + 1;
        ++i;
    }
    i = 0;
    while(i < 64) {
        s += i * 2;
        ++i;
    }
    while(i < 65) {
        s += i * 3;
        ++i;
    }
    while(i < 67) {
        s += i * i * i;
        ++i;
    }
    putint(s);
    return s;
}
