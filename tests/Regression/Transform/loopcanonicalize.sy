int main() {
    int sum = 0;
    for(int i = 0; i < 10; ++i)
        sum += i;
    for(int i = 0; i < 1000; i+=10)
        sum += i;
    for(int i = 0; 10 > i; ++i)
        sum += i;
    for(int i = 0; 1000 > i; i-=-10)
        sum += i;
    for(int i = 1000; i > 0; i-=10)
        sum += i;
    for(int i = 0; i != 1000; i++)
        sum += i;
    for(int i = 0; i <= 1000; i++)
        sum += i;
    int n = getint();
    for(int i = 0; i < n; i++)
        sum += i;
    return sum;
}
int test_sle(int n) {
    int i = 0;
    while(i <= n) ++i;
    return i;
}
