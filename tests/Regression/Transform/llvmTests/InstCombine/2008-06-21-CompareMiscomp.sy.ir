func @test(i32 %var0) -> i32 { NoMemoryRead NoMemoryWrite NoSideEffect Stateless NoRecurse } {
  ^entry:
    i1 %0 = icmp ugt i32 %var0, i32 13;
    i1 %1 = icmp eq i32 %var0, i32 15;
    i1 %2 = and i1 %0, i1 %1;
    i32 %3 = zext i1 %2 to i32;
    i32 %4 = ztrunc i32 %3 to i32;
    ret i32 %4;
}
func @test_logical(i32 %var0) -> i32 { NoMemoryRead NoMemoryWrite NoSideEffect Stateless NoRecurse } {
  ^entry:
    i1 %0 = icmp eq i32 %var0, i32 15;
    i32 %1 = zext i1 %0 to i32;
    i1 %2 = icmp ugt i32 %var0, i32 13;
    i32 %3 = select i1 %2 ? i32 %1 : i32 0;
    i32 %4 = ztrunc i32 %3 to i32;
