int test(int a, int b) {
    if(b == 0) a += 100;
    else if(b == 1) a -= 100;
    else if(b == 2) a *= 100;
    else if(b == 3) a /= 100;
    return a;
}
int testSwitch(int a, int b) {
    switch(b) {
        case 0: a += 100; break;
        case 1: a -= 100; break;
        case 2: a *= 100; break;
        case 3: a /= 100; break;
        default: break;
    }
    return a;
}
