FUNCTION DigitSum :
PARAM v0
IF v0 != #0 GOTO label1
RETURN #0
LABEL label1 :
v1 := v0 / #10
v2 := v1 * #10
v3 := v0 - v2
ARG v1
v4 := CALL DigitSum
v5 := v4 + v3
RETURN v5

FUNCTION main :
READ v6
IF v6 >= #0 GOTO label3
WRITE #-1
RETURN #0
LABEL label3 :
ARG v6
v7 := CALL DigitSum
WRITE v7
RETURN #0

