func @read() -> i32 { NoMemoryRead NoMemoryWrite };
func @write(i32) -> void { NoMemoryRead NoMemoryWrite };
func @add([2 * i32]*) -> i32 { NoMemoryWrite NoSideEffect NoRecurse } {
^entry([2 * i32]* %arg):
    i32* %0 = getelementptr &([2 * i32]* %arg)[i32 0][i32 0];
    i32 %1 = load i32* %0;
    i32* %2 = getelementptr &([2 * i32]* %arg)[i32 0][i32 1];
    i32 %3 = load i32* %2;
    i32 %4 = add i32 %1, i32 %3;
    ret i32 %4;
}
func @main() -> i32 { NoMemoryRead NoMemoryWrite NoRecurse } {
^entry():
    call (i32) -> void @write(i32 1);
    call (i32) -> void @write(i32 3);
    ret i32 0;
}
