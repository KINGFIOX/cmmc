struct Operands {i32 o1, i32 o2};

func @read() -> i32 { NoMemoryRead NoMemoryWrite };
func @write(i32) -> void { NoMemoryRead NoMemoryWrite };
func @add(struct Operands*) -> i32 { NoMemoryWrite NoSideEffect NoRecurse } {
^entry(struct Operands* %arg):
    i32* %0 = getelementptr &(struct Operands* %arg)[i32 0].o1;
    i32 %1 = load i32* %0;
    i32* %2 = getelementptr &(struct Operands* %arg)[i32 0].o2;
    i32 %3 = load i32* %2;
    i32 %4 = add i32 %1, i32 %3;
    ret i32 %4;
}
func @main() -> i32 { NoMemoryRead NoMemoryWrite NoRecurse } {
^entry():
    call (i32) -> void @write(i32 3);
    ret i32 0;
}
